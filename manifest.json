{
  "manifest_version": 2,

  "name": "Back-a-Slash",
  "description": "Simple extension to go up one level in the current website directory",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Up one level."
  }
}
